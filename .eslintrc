{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "airbnb-base"
    ],

    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
      "import"
    ],
    "rules": {
      "no-underscore-dangle": ["error",{"allow":["_id"]}],
      "no-console": "off"
    }

}